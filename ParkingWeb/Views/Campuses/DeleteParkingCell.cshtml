@model ParkingWeb.Data.Entities.ParkingCell

@{
    ViewData["Title"] = "Delete";
}

<h1 class="text-center">Eliminar</h1>
<h3 class="text-danger text-center">¿Esta seguro que desea eliminar esta Celda de Parqueo?</h3>
<div class="mt-4">
    <partial name ="_ParkingCellDetails"/>
    <div class="mt-4 text-center">
        <form asp-action="DeleteParkingCell">
            <input type="hidden" asp-for="Id" />
            <button input type="submit" value="Eliminar" class="btn btn-danger"><i class="fa-solid fa-trash-can"></i> Eliminar</button>
            <a asp-action="DetailsParkingLot" asp-route-id="@Model.ParkingLot.Id" class="btn btn-dark"><i class="fa-solid fa-rotate-left"></i> Regresar</a>
        </form>

    </div>
</div>
